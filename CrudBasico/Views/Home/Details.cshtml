@model Amigo
@{
    ViewBag.Title = "Details";
}
<h1>Details</h1>
<form>
    <div class="form-group">
        <label>Codigo</label>
        <input class="form-control" type="text" value="@Model.id" />
    </div>
    <div class="form-group">
        <label>Nombres</label>
        <input class="form-control" type="text" value="@Model.nombres" />
    </div>
    <div class="form-group">
        <label>Correo</label>
        <input class="form-control" type="text" value="@Model.correo" />
    </div>
    <div class="form-group">
        <label>Provincia</label>
        <input class="form-control" type="text" value="@Model.provincia" />
    </div>
</form>

